[Unit]
Description=INAETICS Provision Service on host port 8080
Documentation=https://github.com/INAETICS

[Service]
ExecStart=/opt/inaetics/fleet/bin/provisioning.sh --hostname=%H --machineId=%m --instanceId=$i --start
ExecStop=/opt/inaetics/fleet/bin/provisioning.sh --hostname=%H --machineId=%m --instanceId=%i --stop

Restart=always
RestartSec=10

#[X-Fleet]
Conflicts=felix@*.service
Conflicts=provisioning.service
